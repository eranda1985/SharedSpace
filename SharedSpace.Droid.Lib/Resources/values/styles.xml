<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="SharedSpaceTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <item name="windowNoTitle">true</item>
    <item name="windowActionBar">false</item>
    <item name="colorPrimary">@color/sharedspace_white</item>
    <item name="colorPrimaryDark">#000000</item>
    <item name="colorAccent">#FF4081</item>
    <item name="windowActionModeOverlay">true</item>
    <item name="android:colorActivatedHighlight">@android:color/transparent</item>
  </style>
  <style name="ExpandableListGroupStyle" parent="@android:style/Widget.ListView">
    <item name="android:textSize">19dp</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:textColor">#505050</item>
  </style>
  <style name="ExpandableListChildStyle" parent="@android:style/Widget.ListView">
    <item name="android:textSize">17dp</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:textColor">#585858</item>
    <item name="android:textStyle">bold</item>
  </style>
  <style name="ExpandableListChildSubStyle" parent="@android:style/Widget.ListView">
    <item name="android:textSize">15dp</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:textColor">#585858</item>
    <item name="android:textStyle">normal</item>
  </style>

</resources>
