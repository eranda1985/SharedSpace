<?xml version="1.0"?>
<package >
  <metadata>
    <id>SharedSpace.ExpandableList</id>
    <version>1.0.3</version>
    <authors>Eranda Galhenage</authors>
    <owners>Eranda Galhenage</owners>
    <projectUrl>https://github.com/eranda1985/SharedSpace</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An expandable list for iOS and Android. Built against Xamarin Forms 3.3.0.967583</description>
    <releaseNotes>Support for Xamarin Forms 3.3. Fixed the arrow directions issue in Android. Fixed the undisposed object issue.</releaseNotes>
    <copyright>Copyright 2018</copyright>
    <tags></tags>
    <dependencies>
      <dependency id="NETStandard.Library" version="2.0.0" />
      <dependency id="Microsoft.NETCore.Platforms" version="1.1.0" />
      <dependency id="Xamarin.Forms" version="3.3.0.967583" />
      <dependency id="FreshEssentials" version="2.1.3" />
      <dependency id="FreshMvvm" version="2.2.4" />
    </dependencies>
  </metadata>
  <files>
    <!--Core-->
    <file src="SharedSpace\bin\Release\netstandard2.0\*.dll" target="lib\netstandard2.0" />
    <!--Xamarin.Android-->
    <file src="SharedSpace.Droid.Lib\bin\Release\*.dll" target="lib\MonoAndroid40" />
    <!--Xamarin.iOS-->
    <file src="SharedSpace.iOS.Lib\bin\Release\*.dll" target="lib\Xamarin.iOS100" />
  </files>
</package>